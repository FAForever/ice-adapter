{
  "name": "faf-ice-adapter",
  "version": "5.0.0",
  "description": "P2P connection relay for Supreme Commander : Forged Alliance based on ICE",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "format": "./node_modules/.bin/tsfmt -r *.ts",
    "test": "./node_modules/.bin/mocha tests --watch",
    "tscompile": "./node_modules/.bin/tsc",
    "bundlejs": "./node_modules/.bin/browserify --no-builtins --bare --no-bf --node --dg true index.js -o ./build/faf-ice-adapter.js"
  },
  "keywords": [
    "Supreme Commander",
    "Forged Alliance",
    "P2P",
    "game",
    "RTS",
    "webrtc"
  ],
  "author": "",
  "license": "GPL-3.0",
  "bin": {
    "faf-ice-adapter": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/faforever/ice-adapter"
  },
  "bugs": {
    "url": "https://github.com/faforever/ice-adapter/issues"
  },
  "files": [
    "*.js"
  ],
  "dependencies": {
    "jayson": "2.0.2",
    "optimist": "0.6.1",
    "winston": "2.3.0",
    "wrtc": "0.0.61",
    "twilio": "2.11.1"
  },
  "devDependencies": {
    "@types/node": "7.0.0",
    "@types/winston": "2.2.0",
    "browserify": "13.3.0",
    "chai": "3.5.0",
    "mocha": "3.2.0",
    "typescript": "2.1.4",
    "typescript-formatter": "4.0.1",
    "uglifyjs": "^2.4.10"
  },
  "preferGlobal": true,
  "private": true
}
